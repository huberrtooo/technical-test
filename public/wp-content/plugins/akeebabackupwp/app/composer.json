{
	"name": "akeeba/solo",
	"type": "project",
	"description": "Akeeba Solo and Akeeba Backup for WordPress",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/akeeba/awf"
		}
	],
	"require" : {
		"php": ">=7.2 <8.3",
		"akeeba/awf": "dev-development",
		"composer/ca-bundle": "^1.3.4",
		"phpmailer/phpmailer": "^6.8.0",
		"ext-curl": "*",
		"ext-mbstring": "*",
		"ext-json": "*"
	},
	"suggest": {
		"ext-zend-opcache": "*",
		"ext-apc": "*",
		"ext-wincache": "*"
	},
	"config": {
		"platform": {
			"php": "7.2.999"
		}
	},
	"autoload" : {
		"psr-4": {
			"Solo\\": "Solo",
			"Solo\\Alice\\": "Solo\\AliceChecks"
		}
	}
}
